KERNEL_ARCH_CFLAGS=    -std=gnu11 \
    -ffreestanding \
    -fno-stack-protector \
    -fno-stack-check \
    -fno-lto \
    -fno-pie \
    -fno-pic \
    -m64 \
    -march=x86-64 \
    -mabi=sysv \
    -mno-80387 \
    -mno-mmx \
    -mno-sse \
    -mno-sse2 \
    -mno-red-zone \
    -mgeneral-regs-only \
    -mcmodel=kernel

KERNEL_ARCH_CPPFLAGS=
KERNEL_ARCH_LDFLAGS=-nostdlib -static -Wl,-m,elf_x86_64 -z max-page-size=0x1000 -Wl,--gc-sections -Wl,--build-id=none
KERNEL_ARCH_LIBS=

KERNEL_ARCH_OBJS=\
$(ARCHDIR)/tty.o \
$(ARCHDIR)/system.o \
$(ARCHDIR)/asm.o \
$(ARCHDIR)/gdt.o \
$(ARCHDIR)/idt.o \
$(ARCHDIR)/isr.o \
$(ARCHDIR)/interrupt.o \
$(ARCHDIR)/pic.o \
$(ARCHDIR)/i8042.o \

